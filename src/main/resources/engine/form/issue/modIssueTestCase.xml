<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
	<object name="issueTestCase" table="t_issue" primary="id" unique="true" order="id desc" autoinc="Y">
	<property name="id" type="Long" length="10" initial="" field="id"/>
	<property name="issueId" type="Long" length="10" initial="" field=""/>
	<property name="name" type="String" length="500" initial="" field="title"/>
	<property name="createTime" type="Date" length="50" initial="" field="create_time"/>
	<property name="createBy" type="String" length="50" initial="" field="createBy"/>
	<property name="updateTime" type="Long" length="50" field="update_time"/>
	<property name="updateBy" type="String" length="50" field="updateBy"/>
	<property name="remark" type="String" length="50" field="remark"/>
	
	<property name="projId" type="Long" length="50" initial="" field=""/>
  </object>       
</model_layer>
</tsoftware>
